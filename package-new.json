{
  "name": "sorita-map-app",
  "version": "1.0.0",
  "description": "Modern location sharing and social mapping application for discovering and sharing places with friends",
  "main": "index.js",
  "author": "SoRita Team",
  "license": "MIT",
  "homepage": "https://github.com/yourorg/sorita-map-app#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourorg/sorita-map-app.git"
  },
  "keywords": [
    "react-native",
    "expo",
    "mapping",
    "social",
    "location",
    "mobile-app"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "build:android:debug": "cd android && gradlew.bat assembleDebug",
    "build:android:release": "cd android && gradlew.bat assembleRelease",
    "build:android:bundle": "cd android && gradlew.bat bundleRelease",
    "eas:build:android": "eas build --platform android",
    "eas:build:ios": "eas build --platform ios",
    "eas:build:all": "eas build --platform all",
    "eas:submit:android": "eas submit --platform android",
    "eas:submit:ios": "eas submit --platform ios",
    "clean": "npx react-native clean && npm run clean:cache",
    "clean:cache": "npm start -- --clear",
    "prebuild": "expo prebuild --clean"
  },
  "dependencies": {
    "@expo/vector-icons": "~14.0.0",
    "babel-preset-expo": "~11.0.0",
    "expo": "~51.0.0",
    "expo-constants": "~16.0.0",
    "expo-crypto": "~13.0.0",
    "expo-dev-client": "~4.0.0",
    "expo-device": "~6.0.0",
    "expo-font": "~12.0.0",
    "expo-image-picker": "~15.0.0",
    "expo-json-utils": "~0.13.0",
    "expo-local-authentication": "~14.0.0",
    "expo-location": "~17.0.0",
    "expo-notifications": "~0.28.0",
    "expo-secure-store": "~13.0.0",
    "expo-status-bar": "~1.12.0",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.0",
    "react-native-safe-area-context": "4.10.0",
    "react-native-screens": "~3.31.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/cli": "^20.0.1",
    "@react-native-community/cli-server-api": "^20.0.1",
    "@react-native/metro-config": "0.74.85",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/react": "~18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "babel-jest": "^29.7.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "detox": "^20.41.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.6",
    "metro-react-native-babel-preset": "0.77.0",
    "prettier": "^3.0.3",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "bugs": {
    "url": "https://github.com/yourorg/sorita-map-app/issues"
  },
  "private": true
}
