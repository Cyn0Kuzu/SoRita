{
  "name": "sorita-map-app",
  "version": "1.0.0",
  "description": "SoRita - Modern location sharing and social mapping application for discovering and sharing places with friends",
  "main": "index.js",
  "author": "SoRita Team",
  "license": "MIT",
  "homepage": "https://github.com/yourorg/sorita-map-app#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourorg/sorita-map-app.git"
  },
  "keywords": [
    "react-native",
    "expo",
    "maps",
    "location",
    "social",
    "firebase",
    "mobile-app"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "build:android:debug": "cd android && gradlew.bat assembleDebug",
    "build:android:release": "cd android && gradlew.bat assembleRelease",
    "build:android:bundle": "cd android && gradlew.bat bundleRelease",
    "eas:build:android": "eas build --platform android",
    "eas:build:ios": "eas build --platform ios",
    "eas:build:all": "eas build --platform all",
    "eas:submit:android": "eas submit --platform android",
    "eas:submit:ios": "eas submit --platform ios",
    "clean": "npx react-native clean && npm run clean:cache",
    "clean:cache": "npm start -- --clear",
    "prebuild": "expo prebuild --clean"
  },
  "dependencies": {
    "@expo/config-plugins": "~9.0.0",
    "@expo/vector-icons": "~14.0.4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-clipboard/clipboard": "^1.16.3",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "babel-preset-expo": "~12.0.0",
    "expo": "~52.0.0",
    "expo-constants": "~17.0.8",
    "expo-crypto": "~14.0.2",
    "expo-dev-client": "~5.0.20",
    "expo-device": "~7.0.3",
    "expo-font": "~13.0.4",
    "expo-image-picker": "~16.0.6",
    "expo-json-utils": "^0.15.0",
    "expo-local-authentication": "~15.0.2",
    "expo-location": "~18.0.10",
    "expo-modules-core": "~2.2.0",
    "expo-notifications": "~0.29.14",
    "expo-secure-store": "~14.0.1",
    "expo-status-bar": "~2.0.1",
    "firebase": "^10.13.2",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "react-native-device-info": "^14.0.4",
    "react-native-edge-to-edge": "1.6.0",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-keychain": "^10.0.0",
    "react-native-maps": "1.18.0",
    "react-native-maps-directions": "^1.9.0",
    "react-native-paper": "^5.10.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-webview": "13.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/cli": "^20.0.1",
    "@react-native/metro-config": "0.76.9",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/react": "~18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "babel-jest": "^29.7.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "detox": "^20.41.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.6",
    "metro-react-native-babel-preset": "0.77.0",
    "prettier": "^3.0.3",
    "react-test-renderer": "18.3.1",
    "typescript": "~5.8.3"
  },
  "bugs": {
    "url": "https://github.com/yourorg/sorita-map-app/issues"
  },
  "private": true
}
